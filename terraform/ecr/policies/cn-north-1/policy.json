      {
        "Sid": "orbit prod pull access",
        "Effect": "Allow",
        "Principal": {
          "AWS": "arn:aws-cn:iam::178588765489:root"
        },
        "Action": [
          "ecr:BatchCheckLayerAvailability",
          "ecr:BatchGetImage",
          "ecr:GetDownloadUrlForLayer",
          "ecr:ListImages"
        ]
      }
    ]
  }
